{
    "name": "riskio/auth0-module",
    "description": "Auth0 features integration into Zend Framework",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "zf2",
        "authentication",
        "oauth"
    ],
    "homepage": "https://github.com/riskio/auth0-module",
    "authors": [
        {
            "name": "Nicolas Eeckeloo",
            "email": "neeckeloo@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "auth0/auth0-php": "^5.0.5",
        "riskio/zf-authentication-auth0": "^1.0",
        "zendframework/zend-stdlib": "~3.1",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Riskio\\Auth0Module\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Riskio\\Auth0ModuleTest\\": "tests/"
        }
    },
    "extra": {
        "zf": {
            "config-provider": "Riskio\\Auth0Module\\ConfigProvider",
            "module": "Riskio\\Auth0Module\\Module"
        }
    }
}
