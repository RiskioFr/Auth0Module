{
    "name": "riskio/auth0-module",
    "description": "Integrate Auth0 features",
    "type": "library",
    "keywords": [
        "zf2",
        "authentication",
        "oauth"
    ],
    "homepage": "https://github.com/riskio/Auth0Module",
    "authors": [
        {
            "name": "Nicolas Eeckeloo",
            "email": "neeckeloo@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.riskio.fr"
        }
    ],
    "require": {
        "php": ">=5.5",
        "auth0/auth0-php": "~3.0",
        "riskio/zf-authentication-auth0": "~0.1",
        "riskio/authentication-module": "~0.1",
        "riskio/oauth2-auth0": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "zendframework/zend-authentication": "~2.5",
        "zendframework/zend-filter": "~2.5",
        "zendframework/zend-http": "~2.5",
        "zendframework/zend-mvc": "~2.5",
        "zendframework/zend-servicemanager": "~2.5",
        "zendframework/zend-session": "~2.5",
        "zendframework/zend-stdlib": "~2.5"
    },
    "autoload": {
        "psr-4": {
            "Riskio\\Auth0Module\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Riskio\\Auth0ModuleTest\\": "tests/"
        }
    }
}
