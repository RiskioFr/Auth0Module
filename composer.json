{
    "name": "riskio/auth0-module",
    "description": "Integrate Auth0 features",
    "type": "library",
    "keywords": [
        "zf2",
        "authentication",
        "oauth"
    ],
    "homepage": "https://github.com/riskio/Auth0Module",
    "authors": [
        {
            "name": "Nicolas Eeckeloo",
            "email": "neeckeloo@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.riskio.fr"
        }
    ],
    "require": {
        "php": ">=5.4",
        "riskio/oauth2-auth0": "~0.1",
        "guzzlehttp/guzzle": "~5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "zendframework/zend-authentication": "~2.3",
        "zendframework/zend-filter": "~2.3",
        "zendframework/zend-http": "~2.3",
        "zendframework/zend-mvc": "~2.3",
        "zendframework/zend-servicemanager": "~2.3",
        "zendframework/zend-session": "~2.3",
        "zendframework/zend-stdlib": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Riskio\\Auth0Module\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Riskio\\Auth0ModuleTest\\": "tests/"
        }
    }
}
